</IngR>
<IngR code="I" name="unsalted butter" unit="cups" qty="1.50">
<IPrp>
(3 sticks, 12 ounces) chilled, cut into 1/2-inch pieces
</IPrp>
</IngR>
<IngR code="I" name="cold water" unit="cup" qty="1.00">
<IPrp>
(8 ounces)
</IPrp>
</IngR>
<IngR code="I" name="salt" unit="teaspoons" qty="2.00"></IngR>
<IngR code="S" name="RICE FILLING:"></IngR>
<IngR code="I" name="butter" unit="tablespoon" qty="1.00">
<IPrp>
or oil
</IPrp>
</IngR>
<IngR code="I" name="diced onion" unit="cup" qty="0.50">
<IPrp>
(2 1/2 ounces)
</IPrp>
</IngR>
