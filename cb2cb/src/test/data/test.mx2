<IngR code="I" name="baking powder" unit="teaspoon" qty="1"></IngR>
<IngR code="I" name="salt" unit="teaspoon" qty="1/4"></IngR>
<IngR code="I" name="stick margarine" unit="tablespoons" qty="2">
<IPrp>
