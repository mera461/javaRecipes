<RcpE name="Marinated Pork-and-Vegetable Kabobs" author="Betsy Fannin, &quot;Grill It With Herbs&quot;">
<Serv qty="4"/>
<PrpT elapsed="0:00"/>
<CatS>
<CatT>
Pork
</CatT>
</CatS>
<IngR code="I" name="unpeeled round red potatoes" unit="small" qty="12">
<IPrp>
(about 1 pound)
</IPrp>
</IngR>
<IngR code="I" name="fresh mushrooms" unit="large" qty="12">
<IPrp>
(about 10 ounces)
</IPrp>
</IngR>
<IngR code="I" name="pork tenderloin" unit="pound" qty="1"></IngR>
<IngR code="I" name="zucchini" unit="(#OP#1/2-inch#CP# slices)" qty="16">
