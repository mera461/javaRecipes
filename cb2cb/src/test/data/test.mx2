<Serv qty="2"/>
<PrpT elapsed="0:00"/>
<CatS>
<CatT>
Poultry
</CatT>
</CatS>
<IngR code="I" name="coarsely chopped red bell pepper" unit="cup" qty="3/4"></IngR>
<IngR code="I" name="slivered fennel bulb" unit="cup" qty="1/2"></IngR>
